' pcm test cases

.define Toys teaselib.Toys
.declare Toys Enum

.declare Applied.by String
.declare Test_Namespace String

.range 1000


' testState

[Action 1000]
.not .state Toys.Collar applied
.state Toys.Collar apply
.range 1001

[Action 1001]
.state Toys.Collar applied 
.state Toys.Collar expired
.state Toys.Collar Remove 
.range 1004

[Action 1004]
.not .state Toys.Collar applied
.state Toys.Collar expired
.range 1005

[Action 1005]
.state Toys.Collar Apply over 01:00"00
.range 1006

[Action 1006]
.state Toys.Collar applied
.not .state Toys.Collar expired
.range 9999


[Action 1008]
.state Toys.Collar applied
.state Toys.Collar expired
.range 1009

[Action 1009]
.state Toys.Collar Remove
.range 9999


' testStateNamespaceAndApplyTags

[Action 1020]
.not .state Toys.Nipple_clamps applied
.range 1021

[Action 1021]
.loadsbd StateTest_subscript.sbd 1020


[Action 1025]
.state Toys.Nipple_clamps applied
.range 1026

[Action 1026]
.state Toys.Nipple_clamps is Test_Namespace
.range 9999


' testStateNamespaceAndApplyTagsInScript

[Action 1030]
.not .state Toys.Collar applied
.not .state Toys.Nipple_clamps applied
.range 1031

[Action 1031]
.state Toys.Collar apply
.range 1032

[Action 1032]
.loadsbd StateTest_subscript.sbd 1030


[Action 1035]
.state Toys.Collar applied
.state Toys.Nipple_clamps applied
.range 1036

[Action 1036]
.state Toys.Collar is Applied.by.StateTest
.state Toys.Nipple_clamps is Applied.by.StateTest_subscript
.range 1037

[Action 1037]
.state Toys.Collar remove
.state Toys.Nipple_clamps remove
.range 1038

[Action 1038]
.not .state Toys.Collar applied
.not .state Toys.Nipple_clamps applied
.not .state Toys.Collar is applied.by.StateTest
.not .state Toys.Nipple_clamps is applied.by.StateTest_subscript
.range 9999


' testRemainingDuration
' - duration is only exact in the chosen unit (set in minutes in the java test code)

[Action 1040]
.state Toys.Nipple_clamps remaining GreaterThan 00:29"00
.state Toys.Nipple_clamps remaining LessOrEqualThan 00:30"00
.range 9999

[Action 1041]
.state Toys.Nipple_clamps remaining GreaterThan -00:01"00
.state Toys.Nipple_clamps remaining LessOrEqualThan 00:00"00
.range 9999

[Action 1042]
.state Toys.Nipple_clamps remaining GreaterThan -00:11"00
.state Toys.Nipple_clamps remaining LessOrEqualThan -00:10"00
.range 9999


' testElapsedDuration
' - duration is only exact in the chosen unit (set in minutes in the java test code)
'   so for tests with frozen time we have to count one up (down for remaining duration)

[Action 1050]
.state Toys.Nipple_clamps elapsed GreaterThan 00:00"00
.state Toys.Nipple_clamps elapsed LessOrEqualThan 00:01"00
.range 9999

[Action 1051]
.state Toys.Nipple_clamps elapsed GreaterThan 00:10"00
.state Toys.Nipple_clamps elapsed LessOrEqualThan 00:11"00
.range 9999

[Action 1052]
.state Toys.Nipple_clamps elapsed GreaterThan 00:30"00
.state Toys.Nipple_clamps elapsed LessOrEqualThan 00:31"00
.range 9999

[Action 1053]
.state Toys.Nipple_clamps elapsed GreaterThan 00:40"00
.state Toys.Nipple_clamps elapsed LessOrEqualThan 00:41"00
.range 9999


' testIfStateConditionOr

[Action 1060]
.ifstate Toys.Nipple_Clamps free or remaining LessOrEqualThan 00:30"00 OR remaining GreaterThan 00:29"00
.unset 1060
.range 9999


[Action 1065]
.unset 1065
.range 1066 1069


[Action 1066]
.state Toys.Nipple_Clamps free
.range 9999

[Action 1067]
.state Toys.Nipple_Clamps applied
.state Toys.Nipple_Clamps remaining LessOrEqualThan 00:30"00
.state Toys.Nipple_Clamps remaining GreaterThan 00:29"00
.range 9999


' testIfStateConditionAnd

[Action 1070]
.ifstate Toys.Nipple_Clamps applied AND expired
.range 9999

[Action 1071]
.ifstate Toys.Nipple_Clamps applied AnD remaining LessOrEqualThan 00:30"00 aNd remaining GreaterThan 00:29"00
.range 9999

[Action 1075]
.ifstate Toys.Nipple_Clamps expired and free
.unset 1075
.range 9999



' proper exit for all test cases
[action 9999]
.unset 9999
.noimage
.quit

