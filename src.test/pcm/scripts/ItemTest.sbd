' pcm test cases

.define Toys teaselib.Toys
.declare Toys Enum

.define Body teaselib.Body
.declare Body Enum

.define Household teaselib.Household
.declare Household Enum

.declare Applied.by String


' testItem
.range 1000


[Action 1000]
.not .item Toys.Collar applied
.item Toys.Collar canApply
.item Toys.Collar apply 
.range 1001

[Action 1001]
.item Body.AroundNeck applied
.item Body.AroundNeck expired
.item Toys.Collar applied
.item Toys.Collar expired
.item Toys.Collar remove
.range 1004

[Action 1004]
.not .item Body.AroundNeck applied
.item Body.AroundNeck expired
.not .item Toys.Collar applied
.item Toys.Collar expired
.range 1005

[Action 1005]
.item Toys.Collar apply over 01:00"00
.range 1006

[Action 1006]
.item Body.AroundNeck applied
.not .item Body.AroundNeck expired
.item Toys.Collar applied
.not .item Toys.Collar expired
.range 9999


[Action 1008]
.item Body.AroundNeck applied
.item Body.AroundNeck expired
.item Toys.Collar applied
.item Toys.Collar expired
.range 1009

[Action 1009]
.item Toys.Collar remove
.range 9999


[Action 1010]
.not .item Household.Clothes_Pegs applied
.not .state Body.OnNipples applied
.item Household.Clothes_Pegs apply to Body.OnNipples 
.range 1011

[Action 1011]
.item Household.Clothes_Pegs applied
.state Body.OnNipples applied
.range 9999


' testStateNamespaceAndApplyTagsInScript

[Action 1030]
.not .item Toys.Collar applied
.not .item Body.AroundNeck applied
.not .item Toys.Nipple_clamps applied
.not .item Body.OnNipples applied
.range 1031

[Action 1031]
.item Toys.Collar apply
.range 1032

[Action 1032]
.loadsbd ItemTest_subscript.sbd 1030


[Action 1035]
.item Toys.Collar applied
.item Toys.Nipple_clamps applied
.range 1036

[Action 1036]
.item Toys.Collar is Applied.by.itemTest
.item Toys.Nipple_clamps is Applied.by.itemTest_subscript
.range 1037

[Action 1037]
.item Toys.Collar remove
.item Toys.Nipple_clamps remove
.range 1038

[Action 1038]
.not .item Toys.Collar applied
.not .item Toys.Nipple_clamps applied
.not .item Toys.Collar is applied.by.ItemTest
.not .item Toys.Nipple_clamps is applied.by.ItemTest_subscript
.range 9999


' testRemainingDuration

[Action 1040]
.item Toys.Nipple_clamps remaining GreaterThan 00:29"00
.item Toys.Nipple_clamps remaining LessOrEqualThan 00:30"00
.range 9999

[Action 1041]
.item Toys.Nipple_clamps remaining GreaterThan -00:01"00
.item Toys.Nipple_clamps remaining LessOrEqualThan 00:00"00
.range 9999

[Action 1042]
.item Toys.Nipple_clamps remaining GreaterThan -00:11"00
.item Toys.Nipple_clamps remaining LessOrEqualThan -00:10"00
.range 9999


' testElapsedDuration
' - duration is only exact in the chosen unit (set in minutes in the java test code)
'   so for tests with frozen time we have to count one up (down for remaining duration)

[Action 1050]
.item Toys.Nipple_clamps elapsed GreaterThan 00:00"00
.item Toys.Nipple_clamps elapsed LessOrEqualThan 00:01"00
.range 9999

[Action 1051]
.item Toys.Nipple_clamps elapsed GreaterThan 00:10"00
.item Toys.Nipple_clamps elapsed LessOrEqualThan 00:11"00
.range 9999

[Action 1052]
.item Toys.Nipple_clamps elapsed GreaterThan 00:30"00
.item Toys.Nipple_clamps elapsed LessOrEqualThan 00:31"00
.range 9999

[Action 1053]
.item Toys.Nipple_clamps elapsed GreaterThan 00:40"00
.item Toys.Nipple_clamps elapsed LessOrEqualThan 00:41"00
.range 9999


' testIfStateConditionOr

[Action 1060]
.item Toys.Nipple_Clamps free or remaining LessOrEqualThan 00:30"00 OR remaining GreaterThan 00:29"00
.unset 1060
.range 9999


[Action 1065]
.unset 1065
.range 1066 1069


[Action 1066]
.item Toys.Nipple_Clamps free
.range 9999

[Action 1067]
.item Toys.Nipple_Clamps applied
.item Toys.Nipple_Clamps is Body.OnNipples
.state Body.OnNipples applied
.item Toys.Nipple_Clamps remaining LessOrEqualThan 00:30"00
.item Toys.Nipple_Clamps remaining GreaterThan 00:29"00
.range 9999


' testIfStateConditionAnd

[Action 1070]
.state Toys.Nipple_Clamps applied AND expired AND is Body.OnNipples
.range 9999

[Action 1071]
.state Toys.Nipple_Clamps applied AnD remaining LessOrEqualThan 00:30"00 aNd remaining GreaterThan 00:29"00 AND is Body.OnNipples
.range 9999

[Action 1075]
.state Toys.Nipple_Clamps expired and free
.unset 1075
.range 9999



' proper exit for all test cases
[action 9999]
.unset 9999
.noimage
.quit

